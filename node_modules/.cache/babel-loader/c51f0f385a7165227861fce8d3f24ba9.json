GAAK,EAAT;AACA,UAAI,EAAC,GAAI,MAAM,CAAC,CAAC,CAAC,GAAC,CAAH,EAAM,OAAN,CAAc,CAAd,CAAD,CAAN,GAA2B,MAAM,CAAC,CAAC,EAAE,GAAC,CAAH,GAAK,CAAN,EAAS,OAAT,CAAiB,CAAjB,CAAD,CAA1C;AAEA,UAAI,QAAO,GAAI,KAAK,MAApB;AACA,WAAK,MAAL,GAAc,CAAd;AACA,WAAK,OAAL,CAAa,IAAb,CAAkB,KAAK,KAAvB;AACA,WAAK,QAAL,CAAc,IAAd,CAAmB,MAAM,CAAC,CAAC,EAAE,GAAC,CAAH,GAAK,CAAN,EAAS,OAAT,CAAiB,CAAjB,CAAD,CAAzB;AACA,WAAK,OAAL,CAAa,IAAb,CAAkB,QAAlB;AAEA,WAAK,OAAL,GAAe,MAAM,CAAC,CAAC,CAAC,GAAC,CAAH,EAAM,OAAN,CAAc,CAAd,CAAD,CAArB;;AACA,aAAM,EAAN,EAAU;AACR,QAAA,EAAC,GAAI,EAAC,GAAI,MAAM,CAAC,CAAC,CAAC,GAAC,CAAH,EAAM,OAAN,CAAc,CAAd,CAAD,CAAhB;;AACA,YAAG,EAAC,GAAI,CAAR,EAAW;AACT,UAAA,OAAO,CAAC,GAAR,CAAY,KAAK,KAAL,GAAa,KAAb,GAAqB,CAAC,EAAE,GAAC,CAAH,GAAK,CAAN,EAAS,OAAT,CAAiB,CAAjB,CAAjC;AACA,eAAK,QAAL,CAAc,IAAd,CAAmB,EAAnB;AACA,eAAK,QAAL,CAAc,IAAd,CAAmB,MAAM,CAAC,CAAC,EAAE,GAAC,CAAH,GAAK,CAAN,EAAS,OAAT,CAAiB,CAAjB,CAAD,CAAzB;AACA,UAAA,QAAO,IAAK,EAAZ;;AACA,cAAI,QAAO,IAAK,CAAhB,EAAmB;AACjB,YAAA,QAAO,GAAI,CAAX;AACF;;AACA,eAAK,OAAL,CAAa,IAAb,CAAkB,QAAlB;AACA,UAAA,EAAC,GAAI,MAAM,CAAC,CAAC,CAAA,GAAI,CAAL,EAAQ,OAAR,CAAgB,CAAhB,CAAD,CAAN,GAA6B,MAAM,CAAC,CAAC,EAAC,GAAI,CAAL,GAAS,CAAV,EAAa,OAAb,CAAqB,CAArB,CAAD,CAAxC;AACA,eAAK,MAAL,IAAe,MAAM,CAAC,EAAD,CAArB;AACA,eAAK,KAAL,IAAc,CAAd;AACA,eAAK,OAAL,CAAa,IAAb,CAAkB,KAAK,KAAvB;AACF,SAbA,MAaO;AACL;AACA;AACA;AACA;AACF;AACF;AACF;AAtCO;AAjBI,CAAf","sourcesContent":["<template>\r\n<div>\r\n  <h2>\r\n  Расчёт кредита (Дифференцированная схема)\r\n  </h2>\r\n  <form @submit.prevent class=\"innerAndButton\">\r\n    <input class=\"colorfun\" type=\"number\" v-model=\"summer\"  placeholder=\"Сумма (руб)\">\r\n    <input class=\"colorfun\" type=\"number\" v-model=\"month\"  placeholder=\"Время (месяцев)\">\r\n    <input class=\"colorfun\" type=\"number\" v-model=\"percent\"  placeholder=\"Проценты\">\r\n    <button class=\"gamburger\" v-on:click=\"currentRes\">Рассчитать!</button>\r\n  </form>\r\n  <div class=\"result\" v-if=\"result > 0\">\r\n    Общая выплата: {{ result }}\r\n  </div>\r\n\r\n<div class=\"table\">\r\n  <div class=\"table__col\">\r\n    Номер платежа\r\n    <div v-for=\"item in counter\" :key=\"item.id\" class=\"table__column1\">\r\n      <div>{{item}}</div>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"table__col\">\r\n    Ежемесячный платёж\r\n    <div v-for=\"item in payments\" :key=\"item.id\" class=\"table__column2\">\r\n      <div>{{item}}</div>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"table__col\">\r\n    Основной платёж\r\n    <div v-for=\"item in payments\" :key=\"item.id\" class=\"table__column2\">\r\n      <div>{{mainpay}}</div>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"table__col\">\r\n    Проценты\r\n    <div v-for=\"item in percents\" :key=\"item.id\" class=\"table__column2\">\r\n      <div>{{item}}</div>\r\n    </div>\r\n  </div>\r\n\r\n\r\n  <div class=\"table__col\">\r\n    Остаток кредита\r\n    <div v-for=\"item in balance\" :key=\"item.id\" class=\"table__column2\">\r\n      <div>{{item}}</div>\r\n    </div>\r\n  </div>\r\n</div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"CredentialCalc\",\r\n  data: () => {\r\n    return {\r\n      summer: Number,\r\n      month: Number,\r\n      percent: Number,\r\n      result: Number,\r\n      count: 1,\r\n      counter: [],\r\n      payments: [],\r\n      mainpay: Number,\r\n      percents: [],\r\n      balance: [],\r\n      monthPay: 0,\r\n    }\r\n  },\r\n  methods: {\r\n    currentRes(){\r\n      let S = this.summer;\r\n      let n = this.month;\r\n      let OD = this.summer;\r\n      let i = Number((this.percent / 100).toFixed(2));\r\n      let k =  12;\r\n      let DP = Number((S/n).toFixed(2)) + Number((OD*i/k).toFixed(2));\r\n\r\n      let balancir = this.summer;\r\n      this.result = 0;\r\n      this.counter.push(this.count);\r\n      this.percents.push(Number((OD*i/k).toFixed(2)));\r\n      this.balance.push(balancir);\r\n\r\n      this.mainpay = Number((S/n).toFixed(2));\r\n      while(OD) {\r\n        OD = OD - Number((S/n).toFixed(2));\r\n        if(OD > 0) {\r\n          console.log(this.count + ':::' + (OD*i/k).toFixed(2))\r\n          this.payments.push(DP);\r\n          this.percents.push(Number((OD*i/k).toFixed(2)));\r\n          balancir -= DP;\r\n          if (balancir <= 0) {\r\n            balancir = 0;\r\n          }\r\n          this.balance.push(balancir);\r\n          DP = Number((S / n).toFixed(2)) + Number((OD * i / k).toFixed(2));\r\n          this.result += Number(DP);\r\n          this.count += 1;\r\n          this.counter.push(this.count);\r\n        } else {\r\n          // this.counter.pop();\r\n          // this.balance.pop();\r\n          // this.percents.pop();\r\n          break;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.innerAndButton {\r\n  margin: 10px;\r\n}\r\n\r\n#area input{\r\n  color: #000000;\r\n}\r\n#area button{\r\n  color: #000000;\r\n}\r\n.gamburger {\r\n  margin-left: 10px;\r\n  color: black;\r\n  background-color: #abb6bd;\r\n  border: none;\r\n  box-shadow: 0 0 10px rgba(0,0,0,0.5);\r\n}\r\n.colorfun {\r\n  background-color: aliceblue;\r\n  color: #000000;\r\n  border: none;\r\n  box-shadow: 0 0 10px rgba(0,0,0,0.5);\r\n  margin: 10px;\r\n}\r\n.result {\r\n  margin-bottom: 40px;\r\n}\r\n.table {\r\n  display: flex;\r\n  width: 100%;\r\n}\r\n.table__col {\r\n  margin-right: 20px;\r\n}\r\n.table__column1 {\r\n  border-bottom: 1px solid gray;\r\n}\r\n.table__column2 {\r\n  border-bottom: 1px solid gray;\r\n}\r\n</style>"],"sourceRoot":""}]}